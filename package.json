{
  "name": "vuepress-plugin-copy-page",
  "version": "1.1.5",
  "description": "A VuePress 2 plugin that adds a Copy Page button to copy page content as Markdown. Perfect for sharing documentation with ChatGPT, Claude, and other AI/LLM tools.",
  "keywords": [
    "vuepress",
    "vuepress-plugin",
    "vuepress2",
    "vuepress-2",
    "copy",
    "copy-page",
    "copy-content",
    "markdown",
    "llm",
    "ai",
    "chatgpt",
    "claude",
    "clipboard",
    "documentation",
    "share",
    "export"
  ],
  "author": "è€Z",
  "license": "MIT",
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./client": {
      "import": "./lib/client/index.js",
      "types": "./lib/client/index.d.ts"
    },
    "./styles/*": "./lib/styles/*"
  },
  "files": [
    "lib",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && cp -r src/styles lib/",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "deps:check": "npm-check-updates",
    "playground:dev": "cd playground && npm run dev",
    "playground:build": "cd playground && npm run build"
  },
  "peerDependencies": {
    "vue": "^3.0.0",
    "vuepress": ">=2.0.0-rc.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-v8": "^2.1.9",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.39.3",
    "@eslint/js": "^9.39.3",
    "jsdom": "^25.0.1",
    "npm-check-updates": "^17.1.18",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0",
    "vitest": "^2.1.9",
    "vue": "^3.5.28",
    "vuepress": "2.0.0-rc.26",
    "eslint-plugin-vue": "^9.33.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhaofutao04/vuepress-plugin-copy-page.git"
  },
  "bugs": {
    "url": "https://github.com/zhaofutao04/vuepress-plugin-copy-page/issues"
  },
  "homepage": "https://vuepress-plugin-copy-page.zhaofutao.cn/",
  "engines": {
    "node": ">=18.0.0"
  },
  "overrides": {
    "markdown-it": "13.0.2",
    "entities": "7.0.1"
  }
}
