{
  "name": "vuepress-plugin-copy-page",
  "version": "1.0.2",
  "description": "A VuePress plugin to copy page content as Markdown for LLMs",
  "keywords": [
    "vuepress",
    "vuepress-plugin",
    "vuepress2",
    "copy",
    "markdown",
    "llm",
    "clipboard"
  ],
  "author": "è€Z",
  "license": "MIT",
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./client": {
      "import": "./lib/client/index.js",
      "types": "./lib/client/index.d.ts"
    },
    "./styles/*": "./lib/styles/*"
  },
  "files": [
    "lib",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && cp -r src/styles lib/",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "deps:check": "npm-check-updates",
    "playground:dev": "cd playground && npm run dev",
    "playground:build": "cd playground && npm run build"
  },
  "peerDependencies": {
    "vue": "^3.0.0",
    "vuepress": ">=2.0.0-rc.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/coverage-v8": "^2.1.8",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.15.0",
    "@eslint/js": "^9.15.0",
    "jsdom": "^25.0.1",
    "npm-check-updates": "^17.1.11",
    "prettier": "^3.4.2",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.15.0",
    "vitest": "^2.1.8",
    "vue": "^3.0.0",
    "vuepress": "2.0.0-rc.26",
    "eslint-plugin-vue": "^9.31.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhaofutao04/vuepress-plugin-copy-page.git"
  },
  "bugs": {
    "url": "https://github.com/zhaofutao04/vuepress-plugin-copy-page/issues"
  },
  "homepage": "https://github.com/zhaofutao04/vuepress-plugin-copy-page#readme"
}
